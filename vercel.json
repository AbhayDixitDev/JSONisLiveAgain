{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/server.js"
    }
  ],
"git": {
    "deploymentEnabled": {
      "main": true
    }
  },
"builds": [
    {
      "src": "./src",
      "use": "@vercel/static-build",
      "config": {
        "github": {
          "autoJobCancellation": true
        }
      }
    }
]
}

